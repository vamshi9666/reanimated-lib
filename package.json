    "babel-jest": "^24.9.0",
    "babel-preset-expo": "^8.0.0",
    "eslint": "^6.7.2",
    "eslint-config-react-native-wcandillon": "2.3.4",
    "eslint-plugin-react": "^7.19.0",
    "react": "^16.8.6",
    "react-native": "^0.61.0",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "^1.4.0",
    "typescript": "^3.6.2"
